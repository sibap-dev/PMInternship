{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_APP": "api/index.py"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30,
      "memory": 1024,
      "runtime": "python3.9"
    }
  }
}
